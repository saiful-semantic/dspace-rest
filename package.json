{
  "name": "dspace-rest",
  "version": "0.0.3",
  "description": "DSpace REST API Client for Node.js",
  "repository": "https://github.com/semanticlib/dspace-rest",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {
    "dspace-cli": "./dist/cli/index.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rimraf dist && tsup src/index.ts src/cli/index.ts --dts --format esm,cjs --outDir dist",
    "test": "mocha --recursive --require ts-node/register"
  },
  "dependencies": {
    "axios": "1.9.0",
    "commander": "^13.1.0",
    "configstore": "^7.0.0"
  },
  "devDependencies": {
    "@types/configstore": "^6.0.2",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.2",
    "@types/sinon": "^17.0.4",
    "mocha": "^11.1.0",
    "sinon": "^20.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "DSpace",
    "REST",
    "DSpace7",
    "DSpace8",
    "DSpace9",
    "Node"
  ]
}
